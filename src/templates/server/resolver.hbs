{{>header}}

import * as Types from '../factories/types{{>tsExtention}}';
import { createServerResolver } from '{{{factories}}}';

{{#each service.operations}}
/**
{{#if deprecated}}
	* @deprecated
{{/if}}
{{#if summary}}
	* {{{escapeComment summary}}}
{{/if}}
{{#if description}}
	* {{{escapeComment description}}}
{{/if}}
{{#each results}}
	 * @returns {{{type}}} {{#if description}}{{{escapeComment description}}}{{/if}}
{{/each}}
*/
export const {{{name}}} = createServerResolver<Types.{{{@root.service.name}}}{{{capitalize name}}}Input, Types.{{{@root.service.name}}}{{{capitalize name}}}Output>({
	path: '{{{path}}}',
	method: '{{{method}}}'{{#if parametersBody}}{{#if parametersBody.mediaType}},
	mediaType: '{{{parametersBody.mediaType}}}'{{/if}}{{/if}},
});
{{/each}}
