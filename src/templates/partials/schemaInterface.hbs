{
    properties: {
{{#if extends}}
    {{#each extends}}
        ...${{{this}}}.properties,
    {{/each}}
{{/if}}
{{#if properties}}
    {{#each properties}}
        {{{name}}}: {{>schema}},
    {{/each}}
{{/if}}
    },
{{#if isReadOnly~}}
    isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired~}}
    isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable~}}
    isNullable: {{{isNullable}}},
{{/if}}
}
