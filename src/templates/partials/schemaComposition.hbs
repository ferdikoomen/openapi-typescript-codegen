{{#if @root}}{
	type: '{{export}}',
{{#if description}}
	description: `{{{escapeDescription description}}}`,
{{/if}}
	contains: [{{#each properties}}{{>schema}}{{#unless @last}}, {{/unless}}{{/each}}],
{{#if isReadOnly}}
	isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired}}
	isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable}}
	isNullable: {{{isNullable}}},
{{/if}}
}{{/if~}}
