{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
{{#unless @root.useUnionTypes}}
export enum {{{name}}} {
    {{#each enum}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{#containsSpaces name}}
    "{{{name}}}" = {{{value}}},
    {{else}}
    {{{name}}} = {{{value}}},
    {{/containsSpaces}}
    {{/each}}
}
{{else}}
export type {{{name}}} = {{#each enum}}{{{value}}}{{#unless @last}} | {{/unless}}{{/each}}{{>isNullable}}
{{/unless}}