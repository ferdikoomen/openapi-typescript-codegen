{{#if @root}}{
	type: 'dictionary',
{{#if link}}
	contains: {{>schema link}},
{{else}}
	contains: {
		type: '{{{base}}}',
	},
{{/if}}
{{#if isReadOnly}}
	isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired}}
	isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable}}
	isNullable: {{{isNullable}}},
{{/if}}
}{{/if~}}
