{{>header}}

import * as Types from '../factories/types.js';
import { createSwrHook } from '{{{factories}}}'

{{#each service.operations}}
export const {{{name}}} = createSwrHook<Types.{{{@root.service.name}}}{{{capitalize name}}}Input, Types.{{{@root.service.name}}}{{{capitalize name}}}Output>({
	path: '{{{path}}}', method: '{{{method}}}'{{#if parametersBody}}{{#if parametersBody.mediaType}}, mediaType: '{{{parametersBody.mediaType}}}'{{/if}}{{/if}}
});
{{/each}}
