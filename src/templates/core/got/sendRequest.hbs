async function sendRequest(options: ApiRequestOptions, url: string): Promise<Response<unknown>> {
    const request: OptionsOfUnknownResponseBody = {
        method: options.method,
        headers: await getHeaders(options),
        body: getRequestBody(options),
        throwHttpErrors: false,
        timeout: OpenAPI.TIMEOUT,
    };
    try {
        return await got(url, request);
    } catch (error) {
        if (error instanceof GotTimeoutError) {
            throw new TimeoutError(url);
        }
        throw error;
    }
}
