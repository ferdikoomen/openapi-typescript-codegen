{{>header}}

import * as Types from '../factories/types.js';
import { createClientResolver } from '{{{factories}}}'

{{#each service.operations}}
/**
{{#if deprecated}}
	* @deprecated
{{/if}}
{{#if summary}}
	* {{{escapeComment summary}}}
{{/if}}
{{#if description}}
	* {{{escapeComment description}}}
{{/if}}
{{#each results}}
	 * @returns {{{type}}} {{#if description}}{{{escapeComment description}}}{{/if}}
{{/each}}
*/
export const {{{name}}} = createClientResolver<Types.{{{@root.service.name}}}{{{capitalize name}}}Input, Types.{{{@root.service.name}}}{{{capitalize name}}}Output>({ path: '{{{path}}}', method: '{{{method}}}' });
{{/each}}
