        "rollup": "3.26.1",
        "rollup-plugin-terser": "7.0.2",
        "rxjs": "7.8.1",
        "ts-node": "10.9.1",
        "tslib": "2.6.0",
        "typescript": "5.1.6",
        "zone.js": "0.13.1"
    },
    "overrides": {
        "node-fetch": "2.6.9",
        "rollup": "3.26.1",
        "typescript": "5.1.6"
    }
}